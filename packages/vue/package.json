{
  "name": "@storyblok/vue-richtext",
  "type": "module",
  "version": "0.0.0",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "keywords": [
    "storyblok",
    "richtext",
    "resolver"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/storyblok-richtext-vue.es.js",
      "require": "./dist/storyblok-richtext-vue.umd.js"
    }
  },
  "main": "./dist/storyblok-richtext-vue.es.js",
  "module": "./dist/storyblok-richtext-vue.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@storyblok/richtext": "workspace:^",
    "@storyblok/vue": "^8.0.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.0",
    "vue-tsc": "^2.0.6"
  }
}
